{
  "content_scripts": [
    {
      "all_frames": true, 
      "js": [
        "content.js"
      ], 
      "match_about_blank": true, 
      "matches": [
        "http://*/*", 
        "https://*/*"
      ], 
      "run_at": "document_end"
    }
  ], 
  "manifest_version": 2, 
  "version": "1",
  "name": "Iframe src change demo", 
  "permissions": [
    "tabs",
    "http://*/*"
  ]
}
